#include <windows.h>

#define PRODUCTVERSION_DEF @PROJECT_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,@PROJECT_VERSION_PATCH@,@PROJECT_VERSION_TWEAK@
#define FILEVERSION_DEF PRODUCTVERSION_DEF
#define PRODUCTVERSION_STR "@PROJECT_VERSION@\0"
#define FILEVERSION_STR PRODUCTVERSION_STR

APP_ICON ICON @APP_ICON@

1 VERSIONINFO
FILEVERSION FILEVERSION_DEF
PRODUCTVERSION PRODUCTVERSION_DEF
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "040904b0"
    BEGIN
      VALUE "CompanyName", "@APP_ORGANIZATION@\0"
      VALUE "ProductName", "@APP_TITLE@\0"
      VALUE "ProductVersion", PRODUCTVERSION_STR
      VALUE "FileVersion", FILEVERSION_STR
      VALUE "FileDescription", "Easy Links\0"
      VALUE "LegalCopyright", "@APP_COPYRIGHT_TEXT@\0"
      VALUE "Contact e-mail", "@APP_AUTHOR_EMAIL@\0"
    END
    BLOCK "080404b0"
    BEGIN
      VALUE "CompanyName", "@APP_ORGANIZATION@\0"
      VALUE "ProductName", "@APP_TITLE@\0"
      VALUE "ProductVersion", PRODUCTVERSION_STR
      VALUE "FileVersion", FILEVERSION_STR
      VALUE "FileDescription", "随心链接\0"
      VALUE "LegalCopyright", "@APP_COPYRIGHT_TEXT@\0"
      VALUE "Contact e-mail", "@APP_AUTHOR_EMAIL@\0"
    END
  END
  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x0409, 1200
  END
END
